<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
    	<referenceContainer name="page.top">
    		<container name="category.view.container" htmlTag="div" htmlClass="category-view container" after="-">
    			<container name="top.content" htmlTag="div" htmlClass="top-content" before="-">
    				<block class="Magento\Theme\Block\Html\Title" name="page.main.title" template="html/title.phtml" before="-"/>
    				<block class="Magento\Theme\Block\Html\Breadcrumbs" name="breadcrumbs" as="breadcrumbs" after="category.image"/>    				
    			</container>
    		</container>
    	</referenceContainer>
        <referenceContainer name="content">
            <block class="Magento\Cms\Block\Page" name="cms_page"/>
        </referenceContainer>
    </body>
</page>
